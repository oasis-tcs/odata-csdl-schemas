                "Department/ID"
            ],
            "$Key": [
                "ID"
            ],
            "ID": {

            },
            "From": {
                "$Type": "Edm.Date"
            },
            "To": {
                "$Type": "Edm.Date"
            },
            "Budget": {
                "$Nullable": true,
