            }
        },
        "Department": {
            "$Kind": "EntityType",
            "$Key": [
                "ID"
            ],
            "ID": {

            },
            "Name": {

            },
            "Employees": {
                "$Kind": "NavigationProperty",
                "$Collection": true,
                "$Type": "OrgModel.Employee"
            },
            "Budget": {
                "$Kind": "NavigationProperty",
                "$Collection": true,
                "$Type": "OrgModel.DepartmentBudget"
            }
        },
        "DepartmentBudget": {
            "$Kind": "EntityType",
            "@Temporal.From": {
                "$Path": "From"
            },
            "@Temporal.To": {
                "$Path": "To"
            },
            "@Temporal.SystemFrom": {
                "$Path": "ChangedAt"
            },
