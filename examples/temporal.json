            ]
        },
        "https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Temporal.V1.json": {
            "$Include": [
                {
                    "$Alias": "Temporal",
                    "$Namespace": "Org.OData.Temporal.V1"
                }
            ]
        }
    },
    "org.example.odata.orgservice": {
        "$Alias": "OrgModel",
        "Employee": {
