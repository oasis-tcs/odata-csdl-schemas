                    "ApplicationTime": {
                        "@type": "https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Temporal.V1.xml#Temporal.GranularityDate",
                        "SupportedQueries": "TimeTravel,TimeSeries"
                    },
                    "SystemTime": {
                        "@type": "https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Temporal.V1.xml#Temporal.GranularityDateTimeOffset",
