                "@Core.Computed": true
            },
            "Department": {
                "$Kind": "NavigationProperty",
                "$Collection": true,
                "$Type": "OrgModel.Department"
            }
        },
        "Default": {
            "$Kind": "EntityContainer",
            "Employees": {
                "$Collection": true,
                "$Type": "OrgModel.Employee",
                "@Temporal.TemporalSupport": {
                    "ApplicationTime": {
                        "@type": "https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Temporal.V1.xml#Temporal.GranularityDate",
                        "SupportedQueries": "TimeTravel,TimeSeries"
                    }
                }
            },
            "Departments": {
                "$Collection": true,
                "$Type": "OrgModel.Department",
