{
    "json.schemas": [
        {
            "fileMatch": [
                "examples/*.json",
                "test/*.json"
            ],
            "url": "./schemas/csdl.schema.json"
        }
    ],
    "xml.catalogs": [
        "./catalog.xml"
    ],
    "mochaExplorer.esmLoader": false,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
}