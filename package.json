{
  "name": "odata-csdl",
  "version": "0.4.3",
  "description": "Convert CSDL XML to CSDL JSON",
  "homepage": "https://github.com/oasis-tcs/odata-csdl-schemas/blob/master/lib/README.md",
  "bugs": "https://github.com/oasis-tcs/odata-csdl-schemas/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/oasis-tcs/odata-csdl-schemas.git"
  },
  "files": [
    "lib/*",
    "schemas/*",
    "catalog.xml"
  ],
  "bin": {
    "odata-csdl-xml2json": "lib/cli.js"
  },
  "main": "lib/xml2json.js",
  "dependencies": {
    "colors": "^1.4.0",
    "minimist": "^1.2.6",
    "sax": "^1.2.4"
  },
  "devDependencies": {
    "ajv": "^8.11.0",
    "c8": "^7.11.0",
    "eslint": "^8.12.0",
    "mocha": "^9.2.2"
  },
  "scripts": {
    "build": "node lib/transform.js",
    "build-java": "javac java/Validator.java",
    "test": "c8 mocha --reporter-option maxDiffSize=0",
    "test-xsd": "java -cp java/xercesImpl.jar;java/org.eclipse.wst.xml.xpath2.processor_1.2.1.jar;java Validator"
  },
